    body#body(style='display: none;')
        div#top_container.wrapper.container-fluid
            div#content
                nav#toptoptop.navbar.navbar-expand-lg.m-0.p-0
                    a.navbar-brand.override(href='/label/pvp')
                        img.d-inline-block(src="/img/zkill.png" width="30" height="30" alt="zKillboard wreck icon")
                        span.d-none.d-md-inline-block.text-dark &nbsp;zKillboard (alpha)
                    ul.navbar-nav.mr-auto
                    ul.navbar-nav
                        li#mails_parsed.pr-3.pl-3
                    ul.navbar-nav
                        li.btn-group
                    ul.navbar-nav
                        li.navbar-item
                            a.nav-link(href='#' onclick='return toggleZTop();')
                                i.bi.bi-server.navbar-item
                    ul.navbar-nav
                        form.form-inline(onsubmit='return false;')
                            input#autocomplete.form-control(type='Search' placeholder='Quick Search' aria-label='Quick Search')
                hr
                div.row
                    div.col-12#ztop
                div#overview.collapse.container-fluid
                    div.d-flex.flex-nowrap.p-0.m-0
                        div#overview-information.p-0.m-0
                        div.m-auto
                        div#overview-statistics.p-0.m-0
                            include stats-box.pug
                    div.row#overview-filters.mb-5
                        div.col-12.mb-2
                            include filters.pug
                    hr.thin
                    div.row.d-flex
                        div.col-xs-12.col-md-8.pr-3
                            div.d-flex
                                div.btn-group.btn-group-sm.mr-2
                                    button.pagefilter.btn.btn-primary(type='button' radio='pagenum') 1
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 2
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 3
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 4
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 5
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 6
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 7
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 8
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 9
                                    button.pagefilter.btn.btn-secondary(type='button' radio='pagenum') 10
                            hr.thin.mt-1
                            div#killlist
                        div.col-xs-12.col-md-4
                            div.d-flex
                                div.m-auto
                                div.btn-group.btn-group-sm
                                    button#stats-epoch-week.tfilter.stats-epoch.btn.btn-secondary(value='hours-01' radio='epoch')
                                        i.bi.bi-hourglass-top
                                    button#stats-epoch-week.tfilter.stats-epoch.btn.btn-secondary(value='hours-24' radio='epoch')
                                        i.bi.bi-hourglass-bottom
                                    button#stats-epoch-week.tfilter.stats-epoch.btn.btn-primary(value='week' radio='epoch') 7
                                    button#stats-epoch-recent.tfilter.stats-epoch.btn.btn-secondary(value='current-month' radio='epoch')
                                        i.bi.bi-calendar3
                                    button#stats-epoch-recent.tfilter.stats-epoch.btn.btn-secondary(value='recent' radio='epoch') 90
                                    button#stats-epoch-recent.stats-epoch.tfilter.btn.btn-secondary(value='prior-month' radio='epoch')
                                        i.bi.bi-calendar-minus
                                    button#stats-epoch-recent.stats-epoch.tfilter.btn.btn-secondary(value='custom-range'  radio='epoch' disabled)
                                        i.bi.bi-clock
                                    button#stats-epoch-alltime.stats-epoch.tfilter.btn.btn-secondary(value='alltime' radio='epoch') âˆž
                            hr.thin.mt-1
                            div#overview-toptens
                    div.m-5
                div#killmail.collapse
                div#other.row.collapse
        div.clearfix.push
